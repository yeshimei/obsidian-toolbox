  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/compress.js": "^1.1.3",
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "builtin-modules": "3.3.0",
    "esbuild": "0.17.3",
    "obsidian": "latest",
    "semantic-release": "^24.1.0",
    "standard-version": "^9.5.0",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "browser-image-compression": "^2.0.2",
    "js-md5": "^0.8.3",
    "openai": "^4.63.0"
